	"@yield": {
		"prefix": "@yield",
		"body": [
			"@yield(\"$id\")"
		],
	},
	"@section": {
		"prefix": "@section",
		"body": [
			"@section($condintion)",
			"\t$content",
			"@endsection"
		],
		"description": "set content"
	},
	"@if": {
		"prefix": "@if",
		"body": [
			"@if($condintion)",
			"\t$content",
			"@endif"
		],
		"description": "condintion"
	},
	"@slot": {
		"prefix": "@slot",
		"body": [
			"@slot($condintion)",
			"\t$content",
			"@endslot"
		],
		"description": "set content"
	},
	"@isset": {
		"prefix": "@isset",
		"body": [
			"@isset($condintion)",
			"\t$content",
