	"$.ajax new":{
		"prefix":"$ajax",
		"body":[
			"e.eventDefault;",
			"var valuesForm = $(${1:form}).serialize(); // get values all form (event when submit)",
			"$.ajax({",
			"\t method: $2 ,",
			"\t data: $3 ,",
			"\t beforeSend: function(){ $4 } ,",
